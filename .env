# Environment configuration for blockchain mode switching
# Options: 'mock' (for development/testing) or 'ganache' (for real blockchain)
BLOCKCHAIN_MODE=ganache
BLOCKCHAIN_API_URL=http://localhost:5002

# AI/ML service
AIML_API_URL=http://localhost:8000
VITE_AIML_API_URL=http://127.0.0.1:8000

# Service health check configuration
HEALTH_CHECK_ENABLED=true
FALLBACK_MODE_ENABLED=true

# Express webhook URL for Django to send anomaly alerts
EXPRESS_WEBHOOK_URL=http://localhost:5001/api/v1/alerts/anomaly

# Other existing environment variables can be added here
PING_MESSAGE=ping